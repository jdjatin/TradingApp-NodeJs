            <!-- phone tab -->
            <div class="phone text">
                <div class="title"><h3>Enter your phone number</h3></div>
              
                <div>
                  <label>Phone number</label>
                  <input type="number" placeholder="enter phone number">
                </div>
                <div>
                  <p>How would you like to receive your confirmation code</p>
                  <div class="check">
                    <input type="checkbox"><label>SMS</label>

                  </div>
                  <div class="check">
                    <input type="checkbox"><label>Phone call</label>

                  </div>
                  <a href="" class="black-btn">Continue</a>
                </div>
                <span><i class="fa fa-solid fa-lock"></i>All data is encrypted</span>
                
            </div>
            <!-- information tab -->
            <div class="information text">
                <div class="title"><h3>Add Profile Information</h3></div>
                <form>
                  <div>
                    <label>First Name</label>
                    <input type="text">
                    <span>Given first name as it appears on your identification.</span>
                  </div>
                  <div>
                    <label>Last Name</label>
                    <input type="text">
                    <span>Given last name as it appears on your identification.</span>
                  </div>
                  <div>
                    <label>Date of Birth</label>
                    <input type="date">
                  
                  </div>
                  <div>
                    <label>Address</label>
                    <input type="text">
                    <span>House no.(apartment),street,city</span>
                  </div>
                  <div>
                    <label>Gender</label>
                    <div class="check">
                      <input type="checkbox" id="Check1" value="Value1" onclick="selectOnlyThis(this.id)" ><p>Male</p>
  
                    </div>
                    <div class="check">
                      <input type="checkbox" id="Check2" value="Value2" onclick="selectOnlyThis(this.id)" ><p>Female</p>
  
                    </div>
                    <div class="check">
                      <input type="checkbox" id="Check3" value="Value3" onclick="selectOnlyThis(this.id)" ><p>Other</p>
  
                    </div>
                  </div>
                  <a href="" class="black-btn">Continue</a>

                </form>
                <span><i class="fa fa-solid fa-lock"></i>All data is encrypted</span>

            </div>
            <!-- economic tab -->
            <div class="economic text">
